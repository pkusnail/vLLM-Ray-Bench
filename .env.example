# Environment Configuration Example
# Copy this to .env and fill in your real values

# Head node IP (your main server)
HEAD_NODE_IP=192.168.1.100

# Worker node IPs (for different cluster sizes)
# For 2-node cluster: only set WORKER_NODE_IP_1
WORKER_NODE_IP_1=192.168.1.101
# For 4-node cluster: set all three worker IPs
WORKER_NODE_IP_2=192.168.1.102
WORKER_NODE_IP_3=192.168.1.103

# Legacy format (comma separated, still supported)
WORKER_NODE_IPS=192.168.1.101,192.168.1.102

# Model configuration
MODEL_NAME=Qwen/Qwen3-32B
MAX_MODEL_LEN=32768
GPU_MEMORY_UTILIZATION=0.8

# Ray configuration
RAY_DASHBOARD_PORT=8265
VLLM_API_PORT=8000

# Cluster settings
TENSOR_PARALLEL_SIZE=8
PIPELINE_PARALLEL_SIZE=2
CLUSTER_NAME=my-vllm-cluster